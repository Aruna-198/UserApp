<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.anveshak.repository.UserMapper">
	<resultMap type="com.anveshak.model.User" id="result">
		<id property="id" column="ID"></id>
		<result property="firstName" column="FIRSTNAME" />
		<result property="lastName" column="LASTNAME"></result>
		<result property="email" column="EMAIL"></result>
		<result property="password" column="PASSWORD"></result>
		<result property="mobileNumber" column="MOBILENUMBER"></result>
		<result property="gender" column="GENDER"></result>
		<result property="dob" column="DOB"></result>
	</resultMap>
	
	</mapper>
	

 
